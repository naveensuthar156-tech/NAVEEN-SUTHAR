n =int(input("enter your naumber:-"))
if(n%7==0):
    print("yes , multiple of 7 ")
else:
    print("no , multiple of 7")





n =int(input("enter your naumber:-"))
if(n%1==0):
    print("yes , multiple of 1 ")
else:
    print("no , multiple of 1")


sure='y'
while sure=='y':
    n =int(input("enter your naumber:-"))
    if(n%10==0):
        print("yes , multiple of 10")
    else:
        print("no , multiple of 10")
sure=input("do you want to continue   y/n??:---")